{
    "name": "buybrain/buybrain-sdk",
    "description": "A PHP SDK for integration with Buybrain",
    "keywords": [
        "buybrain"
    ],
    "homepage": "https://github.com/buybrain/buybrain-sdk-php",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ede Meijer",
            "email": "ede@buybrain.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Buybrain\\": "src/Buybrain/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Buybrain\\": "test/Buybrain/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "buybrain/nervus-php": "^0.2.0"
    },
    "require": {
        "php": "^7.4||^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.2||^7.4"
    }
}
